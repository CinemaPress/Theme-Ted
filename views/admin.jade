        function addPoster(elementId, moviesId) {

            var movies = document.getElementById(elementId);

            movies.onkeyup = function () {
                var movie = document.getElementById(elementId).value.split(',');
                var ms = document.getElementById(moviesId);
                ms.innerHTML = '';
                for (var i = 0; i < movie.length; i++) {
                    if (!movie[i]) continue;
                    ms.innerHTML += '' +
                            '<img src="http://st.kp.yandex.net/images/film_iphone/iphone360_' + movie[i] + '.jpg" class="poster">';
                }
            };

        }

        addPoster('top', 'top-movies');
        addPoster('abuse', 'abuse-movies');