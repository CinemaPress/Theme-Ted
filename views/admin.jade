doctype html
head
    meta(http-equiv='Content-Type', content='text/html; charset=utf-8')
    title= 'Admin - CinemaPress CMS'
    link(rel='shortcut icon', href='/favicon.ico')
    style.
        @import url(https://fonts.googleapis.com/css?family=Open+Sans);

        body {
            background: #272F38 url('data:image/png;base64,') right bottom no-repeat;
            color: #fff;
            margin: 0 0 100px 0;
            padding: 0;
            font-family: "Open Sans";
        }

        .logo {
            margin: 20px;
            text-align: left;
            font-size: 35px;
            color: #77B3D4;
            font-weight: bold;
            text-shadow: 1px 1px 2px #000;
        }

        .ver {
            font-size: 12px;
            color: #3b4755;
            text-shadow: initial;
            font-weight: normal;
        }

        .config {
            background: #334049;
            border: 1px #5c728c solid;
            border-radius: 5px;
            padding: 20px;
            margin: 10px;
            display: none;
        }

        .input {
            background: #273039;
            color: #fff;
            padding: 5px;
            margin: 10px;
            border: 1px #fff solid;
            border-radius: 3px;
        }

        .textarea {
            background: #273039;
            color: #fff;
            padding: 5px;
            margin: 10px;
            border: 1px #fff solid;
            border-radius: 3px;
        }

        .center {
            text-align: center;
            margin: auto;
        }

        .name {
            font-size: 20px;
            color: #516674;
            margin: 10px 0 0 30px;
            cursor: pointer;
        }

        .name:hover {
            color: #fff;
        }

        .poster {
            width: 85px;
            height: 120px;
            border: 1px #ccc solid;
            border-radius: 2px;
            margin: 5px;
        }

        table {
            width: 100%;
        }

        table, td, tr {
            border: 0;
        }

        td:first-child {
            width: 30%;
        }

        td:last-child {
            width: 70%;
        }

        td:last-child input {
            width: 100%;
            margin: 10px 0;
        }

        td:last-child textarea {
            width: 100%;
            margin: 10px 0;
            height: 200px;
        }

        .top {
            position: fixed;
            top: 0;
            right: 0;
            margin: 20px;
        }

        .submit {
            padding: 20px;
            background: #272F38;
            color: #ccc;
            text-decoration: none;
            cursor: pointer;
            border: 1px #5c728c solid;
            border-radius: 5px;
            float: left;
            margin: 0 5px 0 0;
        }

        .help {
            padding: 20px;
            background: #272F38;
            color: #ccc;
            text-decoration: none;
            cursor: pointer;
            border: 1px #5c728c solid;
            border-radius: 5px;
            margin: 0 0 0 5px;
            float: left;
        }

        .submit:hover, .help:hover {
            background: #2e3742;
            color: #fff;
        }

